import strings.encodings.utf8.(UTF8Decoder);
import show.(show);
import vectors.(Vector);

main() {
    for (c in UTF8Decoder(iterator(Vector[UInt8](
        0x00_u8, // nul
        0x41_u8, // A
        0x7F_u8, // del
        0xC2_u8, 0xA0_u8, // U+A0 nbsp
        0xC3_u8, 0xA9_u8, // U+E9 √©
        0xE2_u8, 0x82_u8, 0xAC_u8, // U+20AC ‚Ç¨
        0xF0_u8, 0x9D_u8, 0x8D_u8, 0xA4_u8, // U+1D364 ùç§
    )))) {
        show(c);
    }
}
