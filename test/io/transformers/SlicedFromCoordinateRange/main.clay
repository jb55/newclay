import io.transformers.(SlicedFrom);
import show.(show);
import vectors.(Vector);

main() {
    var xs = Vector[Int](0, 0, 0, 0, 0);

    for (ref x in SlicedFrom(xs, 2))
        x = 1;

    for (x in xs)
        show(x);

    show("--");

    for (x in SlicedFrom(Vector[Int](2, 3, 5, 7, 11), 0))
        show(x);
    show("--");

    for (x in SlicedFrom(Vector[Int](2, 3, 5, 7, 11), 1))
        show(x);
    show("--");

    for (x in SlicedFrom(Vector[Int](2, 3, 5, 7, 11), 4))
        show(x);
    show("--");

    for (x in SlicedFrom(Vector[Int](2, 3, 5, 7, 11), 5))
        show(x);
    show("--");

    for (x in SlicedFrom(Vector[Int](2, 3, 5, 7, 11), 6))
        show(x);
}
