import show.(show);
import test.valuesemantics.(ValueSemanticsLogger);
eatRvalue(rvalue x) {}
nop(x) {}

foo(a?, b?, c?) {
    rvalue x = ValueSemanticsLogger(1);

    if (a?) return;

    if (b?)
        nop(x);
    else
        eatRvalue(x);

    if (c?) return;
}

main() {
    static for (a? in true, false)
        static for (b? in true, false)
            static for (c? in true, false) {
                foo(a?, b?, c?);
                show("--");
            }
}
