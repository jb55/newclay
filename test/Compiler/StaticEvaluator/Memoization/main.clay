import show.(show);
import __primitives__.*;

define Factorial;
overload #Factorial('n) = staticMultiply('n, Factorial(staticSubtract('n, 1)));
overload #Factorial(1) = 1;
overload #Factorial(0) = 1;

define Main;
overload #Main() = Factorial(2), Factorial(3), Factorial(5), Factorial(8);

main() { show(..#Main()); }
